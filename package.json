{
  "name": "jwt-js-decode",
  "version": "1.4.1",
  "description": "",
  "keywords": [],
  "main": "dist/jwt-js-decode.umd.js",
  "module": "dist/jwt-js-decode.esm.js",
  "browser": "dist/jwt-js-decode.min.js",
  "common": "dist/jwt-js-decode.cjs.js",
  "es6": "dist/jwt-js-decode.es6.js",
  "packed": "dist/jwt-js-decode.pkg.min.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "Apache License 2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomitribe/jwt-js-decode.git"
  },
  "scripts": {
    "prebuild": "rimraf dist && rimraf compiled",
    "build": "rollup -c",
    "postbuild": "npm run docs",
    "predocs": "rimraf docs",
    "docs": "typedoc --out docs --target es6 --theme markdown --mode file src/jwt-js-decode.ts",
    "dev": "rollup -c -w",
    "precommit": "lint-staged",
    "pretest": "npm run build && rimraf coverage",
    "test": "npm run jest --",
    "jest": "jest --verbose --no-cache",
    "test:watch": "jest --watch --no-cache",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'"
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "prettier --parser flow --write",
      "git add"
    ],
    "*.ts": [
      "prettier --parser typescript --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/dist/"
    ],
    "modulePaths": [
      "src",
      "node_modules"
    ],
    "collectCoverage": false,
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json"
      }
    }
  },
  "dependencies": {
    "pako": "1.0.10"
  },
  "devDependencies": {
    "@types/jest": "^24.0.20",
    "@types/node": "^12.11.7",
    "@types/pako": "^1.0.1",
    "babel-jest": "^24.9.0",
    "babel-register": "^6.26.0",
    "browserify-sign": "^4.0.4",
    "create-hash": "^1.2.0",
    "create-hmac": "^1.1.7",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.25.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.24.3",
    "subtle": "git+https://github.com/Dexmaster/subtle.git#97bb1842778155a9485df8c0eda84cdfa645468a",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^2.2.11",
    "typescript": "^2.9.1"
  }
}
