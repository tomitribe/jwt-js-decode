{
  "name": "jwt-js-decode",
  "version": "1.6.0",
  "description": "JavaScript library that decodes JSON Web Tokens (JWT) jwt-js-decode the easy way",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/tomitribe/jwt-js-decode.git"
  },
  "license": "Apache-2.0",
  "main": "./dist/jwt-js-decode.umd.js",
  "module": "./dist/jwt-js-decode.esm.js",
  "browser": "./dist/jwt-js-decode.min.js",
  "typings": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist && rimraf compiled",
    "build": "rollup -c --bundleConfigAsCjs",
    "postbuild": "npm run docs",
    "dev": "rollup -c -w",
    "predocs": "rimraf docs",
    "docs": "typedoc --plugin typedoc-plugin-markdown",
    "jest": "jest --verbose --no-cache",
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "precommit": "lint-staged",
    "pretest": "npm run build && rimraf coverage",
    "test": "npm run jest --",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "test:watch": "jest --watch --no-cache"
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "prettier --parser flow --write",
      "git add"
    ],
    "*.ts": [
      "prettier --parser typescript --write",
      "git add"
    ]
  },
  "dependencies": {
    "pako": "2.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.4",
    "@types/pako": "^2.0.0",
    "babel-jest": "^29.6.1",
    "babel-register": "^6.26.0",
    "browserify-sign": "^4.2.1",
    "create-hash": "^1.2.0",
    "create-hmac": "^1.1.7",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "29.6.1",
    "lint-staged": "^13.2.3",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.3",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=15.0.0"
  },
  "common": "./dist/jwt-js-decode.cjs.js",
  "es6": "./dist/jwt-js-decode.es6.js",
  "overrides": {
    "@rollup/pluginutils": {
      "rollup": "$rollup"
    }
  },
  "packed": "./dist/jwt-js-decode.pkg.min.js"
}
