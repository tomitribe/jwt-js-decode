{
  "name": "jwt-js-decode",
  "version": "1.5.1",
  "description": "JavaScript library that decodes JSON Web Tokens (JWT) jwt-js-decode the easy way",
  "keywords": [],
  "main": "./dist/jwt-js-decode.umd.js",
  "module": "./dist/jwt-js-decode.esm.js",
  "browser": "./dist/jwt-js-decode.min.js",
  "common": "./dist/jwt-js-decode.cjs.js",
  "es6": "./dist/jwt-js-decode.es6.js",
  "packed": "./dist/jwt-js-decode.pkg.min.js",
  "typings": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">=15.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomitribe/jwt-js-decode.git"
  },
  "scripts": {
    "prebuild": "rimraf dist && rimraf compiled",
    "build": "rollup -c",
    "postbuild": "npm run docs",
    "predocs": "rimraf docs",
    "docs": "typedoc",
    "dev": "rollup -c -w",
    "precommit": "lint-staged",
    "pretest": "npm run build && rimraf coverage",
    "test": "npm run jest --",
    "jest": "jest --verbose --no-cache",
    "test:watch": "jest --watch --no-cache",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'"
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "prettier --parser flow --write",
      "git add"
    ],
    "*.ts": [
      "prettier --parser typescript --write",
      "git add"
    ]
  },
  "dependencies": {
    "pako": "2.0.4"
  },
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.8",
    "@types/pako": "^2.0.0",
    "babel-jest": "^28.1.3",
    "babel-register": "^6.26.0",
    "browserify-sign": "^4.2.1",
    "create-hash": "^1.2.0",
    "create-hmac": "^1.1.7",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "lint-staged": "^13.0.3",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "ts-jest": "28.0.8",
    "ts-node": "^10.9.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.23.10",
    "typedoc-plugin-markdown": "^3.13.4",
    "typescript": "^4.7.4"
  }
}
